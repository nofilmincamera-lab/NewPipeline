    {
      "label": "Case Study",
      "url_patterns": [
        "/case-stud(y|ies)",
        "/success-stor(y|ies)",
        "/customer-stor(y|ies)",
        "/clients?-stories",
        "/resources?/case[-\\s]?stud(y|ies)",
        "/stories?/customers?",
        "/client(s)?/success",
        "/outcomes?/",
        "/impact/",
        "/caso(s)?-?de-?(éxito|exito|estudio)",
        "/estudo(s)?-?de-?caso",
        "/etude(s)?-?de-?cas"
      ],
      "url_weight": 15,
      "title_patterns": [
        "\\bcase study\\b",
        "\\bsuccess story\\b",
        "\\bcustomer story\\b",
        "\\bhow\\s+\\w+\\s+(achieved|improved|reduced|increased)",
        "\\b(case|client|customer|partner) (study|story|success)\\b",
        "\\b(before|after)\\b",
        "\\b(the|our) impact\\b",
        "\\bresults?\\b",
        "(bank|fintech|telecom|retail|travel|healthcare|utilities|auto|gaming) (case|story)"
      ],
      "title_weight": 10,
      "content_patterns": [
        "\\b(the\\s+)?(challenge|problem|issue)\\b",
        "\\b(the\\s+)?(solution|approach)\\b",
        "\\b(the\\s+)?(results?|outcome|impact)\\b",
        "\\d+%\\s+(increase|decrease|reduction|improvement|growth)",
        "(improved|reduced|increased)\\s+by\\s+\\d+%",
        "(said|stated|explained)\\s+\\w+,?\\s+(ceo|director|manager)",
        "\\b(challenge|objective|goal|baseline)\\b",
        "\\b(approach|solution|program|pilot)\\b",
        "\\b(result(s)?|outcome(s)?|impact)\\b",
        "\\b[A-Za-z ]+ by \\d+%\\b",
        "\\b(dropped|cut|reduced) (AHT|backlog|FCR|attrition)\\b",
        "\\b(increased|improved) (CSAT|NPS|FCR|sales)\\b",
        "\\\"(.*?)\\\"\\s*—\\s*\\w+(,)?\\s*(CEO|COO|VP|Director)",
        "\\b(challenge|solution|results?)\\b"
      ],
      "pattern_weight": 2,
      "min_patterns": 3,
      "structure_signals": {
        "has_metrics": true,
        "has_sections": [
          "challenge",
          "solution",
          "result"
        ],
        "has_quotes": true,
        "min_length": 400,
        "triad_headings:challenge_solution_results": true,
        "metrics_block": true,
        "single_client_logo": true
      },
      "min_score": 20,
      "synonyms": [
        "case study",
        "customer story",
        "success story",
        "transformation",
        "before and after",
        "impact",
        "results",
        "kpi",
        "nps",
        "csat",
        "aht",
        "fcr",
        "sla",
        "attrition"
      ],
      "disambiguators": {
        "prefer_if_kpis_and_triad": [
          "blog"
        ]
      },
      "global_hints": {
        "multilingual_soft_terms": [
          "estudio de caso",
          "caso de éxito",
          "comunicado de prensa",
          "servicios",
